{
  "name": "@johnpc/link-extractor",
  "version": "1.0.0",
  "description": "Extract URLs from currently open browser tabs (Firefox, Chrome, Safari)",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "link-extractor": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx src/index.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "keywords": [
    "firefox",
    "tabs",
    "url",
    "extractor",
    "cli"
  ],
  "author": "johnpc",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnpc/link-extractor.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "cheerio": "^1.1.2",
    "commander": "^14.0.2",
    "lz4-napi": "^2.9.0",
    "snappy": "^7.3.3"
  }
}
